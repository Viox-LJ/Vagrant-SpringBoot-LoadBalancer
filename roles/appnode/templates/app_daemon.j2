[Unit]
Description=springboot
After=syslog.target

[Service]
ExecStart=/usr/bin/java -Duser.timezone=UTC -Dserver.port=9000 -jar /vagrant/spring-boot-sample-hateoas-2.1.0.BUILD-SNAPSHOT.jar
StandardOutput=journal
StandardError=journal
SyslogIdentifier=springboot
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target